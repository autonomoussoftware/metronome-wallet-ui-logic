    "husky": "^1.3.0",
    "lint-staged": "^8.1.3",
    "npm-check": "^5.9.0",
    "prettier": "^1.14.2",
    "react": "^16.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --cache"
    ],
    "src/**/*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
